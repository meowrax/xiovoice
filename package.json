            @media (max-height: 700px) and (max-width:768px) {
                .sidebar {
                    max-height:35vh
                }

                .participants-section {
                    min-height: 50px
                }
            }

            @supports (-webkit-touch-callout:none) {
                .user-controls {
                    padding-bottom: calc(12px + env(safe-area-inset-bottom,20px))
                }

                .chat-input-area {
                    padding-bottom: calc(10px + env(safe-area-inset-bottom,20px))
                }

                .sidebar {
                    padding-top: env(safe-area-inset-top,0)
                }

                .participants-section {
                    -webkit-overflow-scrolling: touch;
                    overscroll-behavior: contain
                }

                .messages-container {
                    -webkit-overflow-scrolling: touch;
                    overscroll-behavior: contain
                }
            }

            .touch-scroll {
                -webkit-overflow-scrolling: touch;
                overscroll-behavior: contain;
                touch-action: pan-y
            }

            .user-context-menu {
                position: fixed;
                background: var(--bg-secondary);
                border: 1px solid var(--border);
                border-radius: 12px;
                padding: 12px;
                min-width: 200px;
                z-index: 3000;
                box-shadow: 0 10px 40px rgba(0,0,0,.5);
                display: none;
                animation: fadeIn .15s ease
            }

            .user-context-menu.show {
                display: block
            }

            .context-menu-header {
                display: flex;
                align-items: center;
                gap: 10px;
                padding-bottom: 10px;
                border-bottom: 1px solid var(--border);
                margin-bottom: 10px
            }

            .context-menu-avatar {
                width: 32px;
                height: 32px;
                border-radius: 50%;
                background: var(--accent-dim);
                display: flex;
                align-items: center;
                justify-content: center;
                color: var(--accent);
                font-weight: 600;
                font-size: .85rem
            }

            .context-menu-name {
                font-weight: 600;
                font-size: .9rem
            }

            .context-menu-item {
                display: flex;
                align-items: center;
                gap: 10px;
                padding: 8px 0
            }

            .context-menu-item label {
                font-size: .8rem;
                color: var(--text-secondary);
                min-width: 70px
            }

            .volume-slider {
                flex: 1;
                -webkit-appearance: none;
                appearance: none;
                height: 4px;
                background: var(--bg-tertiary);
                border-radius: 2px;
                outline: 0
            }

            .volume-slider::-webkit-slider-thumb {
                -webkit-appearance: none;
                appearance: none;
                width: 14px;
                height: 14px;
                background: var(--accent);
                border-radius: 50%;
                cursor: pointer
            }

            .volume-slider::-moz-range-thumb {
                width: 14px;
                height: 14px;
                background: var(--accent);
                border-radius: 50%;
                cursor: pointer;
                border: none
            }

            .volume-value {
                font-family: 'JetBrains Mono',monospace;
                font-size: .75rem;
                color: var(--accent);
                min-width: 35px;
                text-align: right
            }

            .context-menu-overlay {
                position: fixed;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                z-index: 2999;
                display: none;
                background: 0 0;
                pointer-events: none
            }

            .context-menu-overlay.show {
                display: block;
                pointer-events: auto
            }
        </style>
    </head>
